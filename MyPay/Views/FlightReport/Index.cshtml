                        str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Service Charge :</span></td><td class="nk-tb-col"><span>' + response.ServiceCharge + '</span></td></tr>';
                        str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Net Amount (Rs) :</span></td><td class="nk-tb-col"><span>' + response.NetAmount + '</span></td></tr>';
                        str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Ip Address :</span></td><td class="nk-tb-col"><span>' + response.IpAddress + '</span></td></tr>';
                        str += '</table></div>';
                        $("#txnDetail").append(str);
                        $('#dvFlightTxnDetail').modal('show');

                        return false;
                    }
                    else {
                        JsonOutput = "Something went wrong. Please try again later.";
                    }
                },
                failure: function (response) {
                    JsonOutput = (response.responseText);
                },
                error: function (response) {
                    JsonOutput = (response.responseText);
                }
            });
        }
    }

