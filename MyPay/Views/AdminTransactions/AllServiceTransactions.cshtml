<script>

    $('.dropdown-mul-2').dropdown({
        multipleMode: 'label',
        searchable: true,
        choice: function () {
            console.log(arguments, this);
        }
    });

    $(document).ready(function () {
        $("#dvtblist").css("display", "none");
        $('.date-picker').datepicker({
            format: "dd-M-yyyy"
        }).datepicker('setDate', 'today');

        //BindAllServiceDataTable();

    });
    function bindtable() {
        $("#dvtblist").css("display", "block");
        var selectedProvider = "";
        $("#servicelist :selected").each(function () {
            if (selectedProvider != "") {
                selectedProvider = (this.value) + "," + selectedProvider;
            }
            else {
                selectedProvider = (this.value);
            }
        });
        $("#selectedProvider").val(selectedProvider);
        BindAllServiceDataTable();
    };

    function GetTxnDetail(Status, Date, UpdatedDate, MemberId, TxnId, MemberName, ContactNumber, CustomerId, Amount, TypeName, StatusName, VendorType, GatewayStatus, CurrentBalance, PreviousBalance, IpAddress,Reference) {
               $("#singleTxn").html("");
        if (Date != "" & TxnId != "" && MemberId != "") {
            var str = '<div class="table-responsive"><table class="nowrap nk-tb-list nk-tb-ulist table-bordered h-100">';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Date :</span></td><td class="nk-tb-col"><span>' + Date + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Updated Date :</span></td><td class="nk-tb-col"><span>' + UpdatedDate + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Member Id :</span></td><td class="nk-tb-col"><span class="tb-lead">' + MemberId + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Transaction Id :</span></td><td class="nk-tb-col"><span><strong style="color: #f98c45">#' + TxnId + '</strong></span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Name :</span></td><td class="nk-tb-col"><span>' + MemberName + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Contact Number :</span></td><td class="nk-tb-col"><span>' + ContactNumber + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Tracker Id :</span></td><td class="nk-tb-col"><span>' + Reference + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Subscriber   :</span></td><td class="nk-tb-col"><span>' + CustomerId + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Amount (Rs) :</span></td><td class="nk-tb-col"><span class="tb-lead">' + Amount + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Service :</span></td><td class="nk-tb-col"><span>' + TypeName + '</span></td></tr>';
            if (Status == 1) {
                str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Status :</span></td><td class="nk-tb-col"><span class="tb-status text-success">' + StatusName + '</span></td></tr>';
            }
            else if (Status == 3) {
                str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Status :</span></td><td class="nk-tb-col"><span class="tb-status text-danger">' + StatusName + '</span></td></tr>';
            }
            else {
                str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Status :</span></td><td class="nk-tb-col"><span class="tb-status text-orange">' + StatusName + '</span></td></tr>';
            }
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Gateway Name :</span></td><td class="nk-tb-col"><span>' + VendorType + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Gateway Status :</span></td><td class="nk-tb-col"><span>' + GatewayStatus + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Current Balance (Rs) :</span></td><td class="nk-tb-col"><span>' + CurrentBalance + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Previous Balance (Rs) :</span></td><td class="nk-tb-col"><span>' + PreviousBalance + '</span></td></tr>';
            str += '<tr class="nk-tb-item"><td class="nk-tb-col"><span>Ip Address :</span></td><td class="nk-tb-col"><span>' + IpAddress + '</span></td></tr>';
            str += '</table></div>';
            $("#singleTxn").append(str);
            $('#singleTransaction').modal('show');
            return false;
        }
    }

    function exported(e) {
        var MemberId = $("#MemberId").val();
        var ContactNumber = $("#ContactNumber").val();
        var Name = $("#Name").val();
        var TransactionId = $("#TransactionId").val();
        var fromdate = $("#fromdate").val();
        var todate = $("#todate").val();
        var Status = $("#status :selected").val();
        var Service = $("#servicelist :selected").val();
        var DayWise = $("#DayWise :selected").val();
        var Reference = $("#Reference").val();
        var VendorType = $("#VendorTypes").val();
        if (MemberId == "") {
            MemberId = 0;
        }
        $.ajax({
            type: "POST",
            url: '@Url.Action("ExportExcelAllServiceTxn", "AdminTransactions")', //call your controller and action
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: false,
            data: '{"MemberId":"' + parseInt(MemberId) + '","ContactNo":"' + ContactNumber + '","Name":"' + Name + '","TxnId":"' + TransactionId + '","FromDate": "' + fromdate + '", "ToDate": "' + todate + '", "Status": "' + Status + '","DayWise":"' + DayWise + '","Service":"' + Service + '","Reference":"' + Reference + '","VendorType":"' + VendorType + '"}',

        }).done(function (data) {
            //get the file name for download
            if (data.fileName != "") {
                //use window.location.href for redirect to download action for download the file
                window.location.href = "@Url.RouteUrl(new { Controller = "AdminTransactions", Action = "Download" })/?fileName=" + data.fileName;
            }
        });
    }


    function AllServiceTxnexported(obj, e) {
        var MemberId = $("#MemberId").val();
        var ContactNumber = $("#ContactNumber").val();
        var Name = $("#Name").val();
        var TransactionId = $("#TransactionId").val();
        var fromdate = $("#fromdate").val();
        var todate = $("#todate").val();
        var Status = $("#status :selected").val();
        var Service = $("#servicelist :selected").val();
        var DayWise = $("#DayWise :selected").val();
        var TrackerId = $("#Reference").val();
        var VendorType = $("#VendorTypes").val();
        var Service = $("#selectedProvider").val();
        if (MemberId == "") {
            MemberId = 0;
        }

        if (window.location.href.indexOf($("#hdndumpurl").val()) > -1) {
            if (fromdate == "") {
                alert("Please select Start Date");
                return false;
            }
            else if (todate == "") {
                alert("Please select End Date");
                return false;
            }
            else if (fromdate != "" && todate != "") {
                var enddate = new Date(todate);
                var startdate = new Date(fromdate);
                var difference = enddate - startdate;
                var totaldays = Math.ceil(difference / (1000 * 3600 * 24));
                if (totaldays > 5) {
                    alert("You cannot export report more than 5 days. So please select StartDate and EndDate accordingly.");
                    return false;
                }
                else {
                    alert("Your exported file is being generated. Please download the new excel after 15 mins. Please note that you can request to generate new file after 4 hrs.");
                    //$("#olduserexport").attr('disabled', true);
                    $("#AllServiceTxnexport").hide();
                }
            }
        }
        $.ajax({
                        type: "POST",
                        url: '@Url.Action("ExportExcelAllServiceTxnDump", "AdminTransactions")', //call your controller and action
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        async: true,
                        data: '{"Service":"' + Service + '","FromDate": "' + fromdate + '", "ToDate": "' + todate + '","MemberId":"' + parseInt(MemberId) + '","ContactNo":"' + ContactNumber + '","Name":"' + Name + '","TxnId":"' + TransactionId + '", "GatewayTxnId": "' + GatewayTxnId + '","TrackerId": "' + TrackerId + '","Status": "' + Status + '","DayWise":"' + DayWise + '","VendorType":"' + VendorType + '"}',
                    }).done(function (data) {
                        //get the file name for download
                        if (data.fileName != "") {
                        }
                        else {
                            alert(data.errorMessage);
                        }
                    });
    }

    function AllServiceTxndownload(e) {
        $.ajax({
            type: "POST",
            url: '@Url.Action("DownloadFileName", "AdminTransactions")', //call your controller and action
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: false,
            data: '',

        }).done(function (data) {
            //get the file name for download
            if (data != "" && data.fileName != "") {
                //use window.location.href for redirect to download action for download the file
                window.location.href = "@Url.RouteUrl(new { Controller = "AdminTransactions", Action = "DownloadFile" })?fileName=" + data.fileName;
            }
            else {
                alert("no file found to download");
            }
        });
    }
</script>
