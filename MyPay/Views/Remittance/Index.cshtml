@model MyPay.Models.Add.AddRemittanceUser
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .select2-container--default .select2-selection--single .select2-selection__rendered {
        color: #526484;
        line-height: 2rem;
        padding: 0.4375rem calc(39px + 2px) 0.4375rem 1rem;
    }

    .select2-container--default .select2-selection--single {
        height: calc(42px + 2px);
    }

    .gallery-image.popup-image {
        min-height: inherit;
        height: inherit;
    }

    .custom-file-inputrounded-top {
        max-width: 100px;
        max-height: 100px;
    }

    .personal-block.nk-block,
    .personal-block .card.card-bordered {
        height: 100%;
    }

    .imgupload .custom-file-label,
    .imgupload .custom-file-label::after {
        height: calc(42px + 2px);
        line-height: 27px;
    }

    .imgupload .custom-file-input,
    .imgupload .custom-file {
        height: calc(42px + 2px);
    }

    .imgupload {
        display: flex;
        align-items: flex-start;
        position: relative;
    }

        .imgupload label.form-label {
            position: absolute;
            top: 0px;
            left: 100px;
        }

        .imgupload .custom-file-inputrounded-top {
            max-width: 85px;
            margin-right: 20px;
            max-height: 80px;
            object-fit: cover;
        }

        .imgupload .custom-file.text-left {
            margin-top: 35px;
        }
</style>
<div class="nk-content ">
    <div class="container-fluid">
        <div class="nk-content-inner">
            <div class="nk-content-body">
                <div class="nk-block-head nk-block-head-sm">
                    <div class="nk-block-between">
                        <div class="nk-block-head-content">
                            <h3 class="nk-block-title page-title mb-4">@(Model.Id == 0 ? "Add":"Edit") Remittance Partner</h3>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="nk-block personal-block">
                                <div class="card card-bordered">
                                    <div class="card-aside-wrap">
                                        <div class="card-inner">
                                            <div class="nk-block">
                                                <div class="nk-block-head">
                                                    <div class="form-group" style="color:red">
                                                        @ViewBag.Message
                                                    </div>
                                                    <div class="form-group" style="color:green">
                                                        @ViewBag.SuccessMessage
                                                    </div>
                                                </div>
                                                <!-- .nk-block-head -->
                                                <div class="nk-block-head nk-block-head-line mb-2">
                                                    <h5 class="card-title">Personal Detail</h5>
                                                </div>
                                                <div class="row gy-4">

                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Contact Name <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.ContactName, new { @class = "form-control form-control-lg", @onkeypress = "return IsAlphabetOnly(event);" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Mobile Number <strong class="text-danger">*</strong></label>

                                                                @if (Model.Id == 0)
                                                                {
                                                                    @Html.TextBoxFor(m => m.ContactNo, new { @class = "form-control form-control-lg", @maxlength = 10, @onkeypress = "return isNumberKey(this, event);" }) }
                                                                else
                                                                {
                                                                    @Html.TextBoxFor(m => m.ContactNo, new { @class = "form-control form-control-lg", @maxlength = 10, @onkeypress = "return isNumberKey(this, event);", @readonly = "true" })}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Email Id <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.EmailID, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">User Name <strong class="text-danger">*</strong></label>
                                                                @if ((Model.Id == 0))
                                                                {
                                                                    @Html.TextBoxFor(m => m.UserName, new { @class = "form-control form-control-lg" }) }
                                                                else
                                                                {
                                                                    @Html.TextBoxFor(m => m.UserName, new { @class = "form-control form-control-lg", @readonly = "true" })}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Country <strong class="text-danger">*</strong></label>
                                                                @Html.DropDownListFor(m => m.CountryId, ViewBag.CountryId as SelectList, "", new { @class = "form-select form-control form-control-lg" })
                                                                @Html.HiddenFor(m => m.CountryName)
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Address <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.Address, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">State <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.State, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">City <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.City, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Zip Code</label>
                                                                @Html.TextBoxFor(m => m.ZipCode, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Password<strong class="text-danger">*</strong> </label>
                                                                <div class="form-control-wrap">
                                                                    @{
                                                                        <a href="javascript:void(0);" class="form-icon form-icon-right passcode-switch md" data-target="Password">
                                                                            <em class="passcode-icon icon-show icon ni ni-eye"></em>
                                                                            <em class="passcode-icon icon-hide icon ni ni-eye-off"></em>
                                                                        </a>
                                                                        @Html.TextBoxFor(m => m.Password, new { @class = "form-control form-control-lg", @maxlength = 20, @value = @ViewBag.Password }) }
                                                                    <input type="hidden" id="hdnPassword" value="@ViewBag.Password" />

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <label class="form-label">Date Of Birth <strong class="text-danger">*</strong></label>
                                                            <div class="form-control-wrap">
                                                                @Html.TextBoxFor(m => m.DateOfBirth, new { @class = "form-control form-control-lg form-control-outlined date-picker", @autocomplete = "off", @readonly = "readonly" })
                                                                <div class="form-icon form-icon-right md">
                                                                    <em class="icon ni ni-calendar-alt"></em>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Gender<strong class="text-danger">*</strong></label>
                                                                @Html.DropDownListFor(m => m.GenderType, ViewBag.GenderType as SelectList, new { @class = "form-select form-control form-control-lg" })
                                                                @Html.HiddenFor(m => m.GenderType)
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">FeeType<strong class="text-danger">*</strong></label>
                                                                @Html.DropDownListFor(m => m.FeeType, ViewBag.FeeType as SelectList, new { @class = "form-select form-control form-control-lg", @onchange = "FeeTypeChanged()" })
                                                                @Html.HiddenFor(m => m.FeeType)
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6" id="dvFixedFee" style="display:none;">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Fee<strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.Fee, new { @class = "form-control form-control-lg", @onkeypress = "return isNumberKey(this, event);" })
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Markup Conversion Rate<strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.MarginConversionRate, new { @class = "form-control form-control-lg", @onkeypress = "return isNumberKey(this, event);" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div><!-- .nk-block -->


                                        </div><!-- .card-content -->
                                    </div><!-- .card-aside-wrap -->
                                </div><!-- .card -->
                            </div><!-- .nk-block -->
                        </div><!--col-md-6-->
                        <div class="col-md-6">
                            <div class="nk-block">
                                <div class="card card-bordered">
                                    <div class="card-aside-wrap">
                                        <div class="card-inner">
                                            <div class="nk-block">

