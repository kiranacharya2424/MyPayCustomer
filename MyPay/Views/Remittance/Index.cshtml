                                                                @Html.DropDownListFor(m => m.AddressProofType, ViewBag.AddressProofType as SelectList, new { @class = "form-select form-control form-control-lg" })
                                                                @Html.HiddenFor(m => m.AddressProofType)
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12 col-sm-12">
