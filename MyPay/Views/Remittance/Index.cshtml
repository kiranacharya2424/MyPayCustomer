                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Success URL</label>
                                                                @Html.TextBoxFor(m => m.SuccessURL, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Cancel URL</label>
                                                                @Html.TextBoxFor(m => m.CancelURL, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">WebSite <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.WebsiteURL, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Ip Address <strong class="text-danger">*</strong></label>
                                                                @Html.TextBoxFor(m => m.MerchantIpAddress, new { @class = "form-control form-control-lg" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group text-center">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Company Logo</label>
                                                                <div class="">

                                                                    <a class="gallery-image popup-image" href="@Model.Image">
                                                                        @if (string.IsNullOrEmpty(Model.Image))
                                                                        {
                                                                            <img ID="targetMerchantImage" class="custom-file-inputrounded-top" width="120" src="~/Content/assets/Images/noimageblank.png" alt=""> }
                                                                        else
                                                                        {

                                                                            <img ID="targetMerchantImage" class="custom-file-inputrounded-top" width="200" src="@Model.Image" alt="">}
                                                                    </a>
                                                                    @Html.HiddenFor(m => m.Image)
                                                                </div>
                                                                <div class="custom-file text-left mt-3">
                                                                    <input type="file" multiple class="custom-file-input" id="MerchantImageFile" onchange="PreviewImage(this,'MerchantImage',5000)" name="MerchantImageFile">
                                                                    <label class="custom-file-label" for="customMultipleFiles">Choose files</label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-6">
                                                        <div class="form-group text-center">
                                                            <div class="form-control-wrap">
                                                                <label class="form-label">Company License Doc </label>
                                                                <div class="">

                                                                    <a class="gallery-image popup-image" href="@Model.AddressProofImage">
                                                                        @if (string.IsNullOrEmpty(Model.AddressProofImage))
                                                                        {
                                                                            <img ID="targetAddressProofImage" class="custom-file-inputrounded-top" width="120" src="~/Content/assets/Images/noimageblank.png" alt=""> }
