                                                                        @Html.HiddenFor(m => m.AdditionalDoc)
                                                                    </div>
                                                                    <div class="custom-file text-left">
                                                                        <input type="file" multiple class="custom-file-input" id="AdditionalDocFile" onchange="PreviewImage(this,'AdditionalDoc',5000)" name="AdditionalDocFile">
                                                                        <label class="custom-file-label" for="customMultipleFiles">Choose files</label>
                                                                    </div>
                                                                </div><!--imgupload-->
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div><!--row-->
                                            </div><!--col-md-6-->


                                        </div>
                                    </div><!-- .nk-block -->
                                    <div class="nk-block p-0">
                                        <div class="row">
                                            <div class="col-lg-12 col-sm-6">
                                                <div class="form-group">
                                                    <div id="dvMessage" class="form-group" style="color:red">
                                                        @ViewBag.Message
                                                    </div>
                                                    <button type="submit" class="btn btn-primary text-center mr-2" id="btnSubmitMerchant">Save</button>
                                                    <button type="button" onclick="window.location='/Remittance/RemittanceList'" class="btn btn-light text-center mr-2">List</button>

                                                </div>
                                            </div>
                                        </div>
                                    </div><!-- .nk-block -->
                                </div><!-- .card-content -->
                            </div><!-- .card-aside-wrap -->
                        </div><!-- .card -->
                    </div><!-- .nk-block -->
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Content/assets/js/userscripts.js"></script>
<script>
    $(function () {
        $("#CountryId").change(function () {
            var value = $("#CountryId option:selected").text();
            $("#CountryName").val(value);

        });
    });
    $(function () {
        $("#FromCurrencyId").change(function () {
            var value = $("#FromCurrencyId option:selected").text();
            $("#FromCurrencyCode").val(value);
        });
    });
    $(function () {
        $("#ProofType").change(function () {
            debugger;
            var value = $("#ProofType option:selected").val();
            if (value == "2" || value == "3") {
                $("#dvBackImage").css("display", "none");
            }
            else {
                $("#dvBackImage").css("display", "");
            }
        });
    });
    $("#Password").val($("#hdnPassword").val());
    $("#Password").attr('type', 'password');
    $(document).ready(function () {
        $('#Document-expiry').datepicker({
            startDate: '+0d'
        })
        if ($("#CountryName").val() == "") {
            $("#CountryId").val("216");
            $("#CountryName").val("Nepal");
        }
        var address = $("#AddressProofType option:selected").text();
        $("#otherProof").hide();
        if (address == "Other") {
            $("#otherProof").show();
        }
    });
    $(function () {
        $("#AddressProofType").change(function () {
            var address = $("#AddressProofType option:selected").text();
            $("#otherProof").hide();
            if (address == "Other") {
                $("#otherProof").show();
            }

        });
    });

    var specialKeys = new Array();
    specialKeys.push(8);  //Backspace
    specialKeys.push(9);  //Tab
    specialKeys.push(46); //Delete
    specialKeys.push(36); //Home
    specialKeys.push(35); //End
    specialKeys.push(37); //Left
    specialKeys.push(39); //Right
    specialKeys.push(32); //Space
    function IsAlphabetOnly(e) {
        var keyCode = e.keyCode == 0 ? e.charCode : e.keyCode;
        var ret = ((keyCode >= 65 && keyCode <= 90) || (keyCode >= 97 && keyCode <= 122) || (specialKeys.indexOf(e.keyCode) != -1));
        return ret;
    }

    function FeeTypeChanged() {
        if ($("#FeeType").val() == "1") {
            $("#dvFixedFee").show();
        }
        else {
            $("#dvFixedFee").hide();
        }
    }
    $(document).ready(function () {
        FeeTypeChanged();
        $("#btnSubmitMerchant").click(function () {
            if ($("#Image").val() == "") {
                $("#Image").val($("#MerchantImageFile").val())
            }
            if ($("#AddressProofImage").val() == "") {
                $("#AddressProofImage").val($("#AddressProofImageFile").val())
            }
            if ($("#IDProofFrontImage").val() == "") {
                $("#IDProofFrontImage").val($("#IDProofFrontImageFile").val())
            }
            if ($("#IDProofBackImage").val() == "") {
                $("#IDProofBackImage").val($("#IDProofBackImageFile").val())
            }
            if ($("#AdditionalDoc").val() == "") {
                $("#AdditionalDoc").val($("#AdditionalDocFile").val())
            }

            var contactName = $("#ContactName").val();
            var companyDirector = $("#CompanyDirectorname").val();
            var contactNo = $("#ContactNo").val();
            var emailID = $("#EmailID").val();
            var pattern = /^[a-zA-Z ]+$/;
            var patternMobile = /^[0-9]+$/;
            var websiteUrl = $("#WebsiteURL").val();
            var successUrl = $("#SuccessURL").val();
            var cancelUrl = $("#CancelURL").val();

            $("#dvMessage").html("");
            if ($("#ContactName").val() == "") {
                $("#dvMessage").html("Please enter Contact Name ");
                return false;
            }

            else if (!pattern.test(contactName)) {
                $("#dvMessage").html("Contact name allow alphabet only");
                return false;
            }
            else if ($("#ContactNo").val() == "") {
                $("#dvMessage").html("Please enter Contact No");
                return false;
            }

            else if (!patternMobile.test(contactNo)) {
                $("#dvMessage").html("Please enter valid contact number");
                return false;
            }

            else if ($("#EmailID").val() == "") {
                $("#dvMessage").html("Please enter Email ID");
                return false;

            }

            else if (!isEmail(emailID)) {
                $("#dvMessage").html("Please enter a valid Email ID");
                return false;
            }

            else if ($("#UserName").val() == "") {
                $("#dvMessage").html("Please enter User  Name");
                return false;

            }
            else if ($("#Address").val() == "") {
                $("#dvMessage").html("Please enter Address   ");
                return false;

            }
            else if ($("#State").val() == "") {
                $("#dvMessage").html("Please enter State   ");
                return false;

            }
            else if ($("#City").val() == "") {
                $("#dvMessage").html("Please enter City   ");
                return false;

            }
            else if ($("#Password").val() == "") {
                $("#dvMessage").html("Please enter Password   ");
                return false;
            }
            else if ($("#DateOfBirth").val() == "") {
                $("#dvMessage").html("Please Select  Date Of Birth");
                return false;
            }
            else if ($("#Gender").val() == "") {
                $("#dvMessage").html("Please Select Gender");
                return false;
            }
            else if ($("#ProofType option:selected").val() == "0") {
                $("#dvMessage").html("Please Select Proof Type");
                return false;
            }
            else if ($("#DocumentNo").val() == "") {
                $("#dvMessage").html("Please Enter Document No");
                return false;
            }
            else if ($("#DocumentExpiryDate").val() == "") {
                $("#dvMessage").html("Please Select Document Expiry Date");
                return false;
            }

            else if ($("#AddressProofType option:selected").val() == "0") {
                $("#dvMessage").html("Please Select Address Proof Type");
                return false;
            }



            else if ($("#OrganizationName").val() == "") {
                $("#dvMessage").html("Please enter Organization Name");
                return false;

            }
            else if ($("#CompanyDirectorname").val() == "") {
                $("#dvMessage").html("Please Select Company Director name");
                return false;
            }
            else if (!pattern.test(companyDirector)) {
                $("#dvMessage").html("Company Director name allow alphabet only");
                return false;
            }
            else if ($("#CompanyRegistrationNo").val() == "") {
                $("#dvMessage").html("Please Enter  Company Registration No");
                return false;
            }
            else if ($("#FromCurrencyId").val() == "0") {
                $("#dvMessage").html("Please Select From Currency");
                return false;
            }

            else if ($("#WebsiteURL").val() == "") {
                $("#dvMessage").html("Please enter WebsiteURL   ");
                return false;

            }
            else if ((!websiteUrl.startsWith("http://")) && (!websiteUrl.startsWith("https://"))) {
                $("#dvMessage").html("Please enter valid WebsiteURL");
                return false;

            }
            else if ($("#MerchantIpAddress").val() == "") {
                $("#dvMessage").html("Please enter MerchantIpAddress   ");
                return false;

            }

            else if ($("#Image").val() == "") {
                $("#dvMessage").html("Please Select Company Logo  Image");
                return false;
            }
            else if ($("#AddressProofImage").val() == "") {
                $("#dvMessage").html("Please Select Company License  Doc");
                return false;
            }

            else if ($("#IDProofFrontImage").val() == "") {
                $("#dvMessage").html("Please Select ID Proof Front Image");
                return false;
            }
            
            else if ($("#AdditionalDoc").val() == "") {
                $("#dvMessage").html("Please Select Doc Image");
                return false;
            }
            else if ($("#AddressProofType option:selected").text() == "Other") {
                if ($("#OtherProofType").val() == "") {
                    $("#dvMessage").html("Please enter other proof type");
                    return false;
                }
            }
            else if (((successUrl != "")) && ((!successUrl.startsWith("http://")) && (!successUrl.startsWith("https://")))) {
                $("#dvMessage").html("Please enter valid SuccessUrl");
                return false;
            }
            else if (((cancelUrl != "")) && ((!cancelUrl.startsWith("http://")) && (!cancelUrl.startsWith("https://")))) {
                $("#dvMessage").html("Please enter valid CancelUrl");
                return false;
            }
            else if ($("#Fee").val() == "") {
                $("#dvMessage").html("Please enter Fee");
                return false;
            }
            else if ($("#MarginConversionRate").val() == "") {
                $("#dvMessage").html("Please enter Margin Conversion Rate");
                return false;
            }
            else if ($("#ProofType option:selected").val() != "2" || $("#ProofType option:selected").val() != "3" ) {
                if ($("#IDProofBackImage").val() == "") {
                $("#dvMessage").html("Please Select ID Proof Back Image");
                return false;
            }
            }
            else {
                $("#AjaxLoader").show();
            }
        });
    });
</script>
