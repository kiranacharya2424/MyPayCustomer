@model MyPay.Models.Add.AddAgent
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .tab button {
        width: 49%;
        border: none;
        outline: none;
        cursor: pointer;
        color: rgb(255 255 255 / 72%);
        Background-color: #93979f;
        font-size: 17px;
    }

        .tab button:hover {
            color: white;
        }

        .tab button.active {
            background-color: #E45415;
            border-bottom-color: #E45415;
            /*        color: rgb(255 255 255 / 72%);*/
        }
</style>
<div class="nk-content ">
    <div class="container-fluid">
        <div class="nk-content-inner">
            <div class="nk-content-body">
                <div class="nk-block-head nk-block-head-sm">
                    <div class="nk-block-between">
                        <div class="nk-block-head-content">
                            <h3 class="nk-block-title page-title">Add Agent</h3>
                        </div>
                    </div>
                    <div class="nk-block">
                        <div class="card card-bordered">
                            <div class="card-aside-wrap">
                                <div class="card-inner">
                                    <div class="nk-block">
                                        <div class="nk-block-head">
                                            <div class="form-group" style="color:red">
                                                @ViewBag.Message
                                            </div>
                                            <div class="form-group" style="color:green">
                                                @TempData["Message"]
                                            </div>
                                        </div><!-- .nk-block-head -->
                                        <div class="nk-block-head nk-block-head-line mt-3 mb-2">
                                            <h6 class="title overline-title text-base">Agent Management Detail</h6>
                                        </div>
                                        @using (Html.BeginForm("AddAgent", "Agent", new { }, FormMethod.Post, new { @role = "form", @id = "AddAgentform" }))
                                        {
                                            @*<input type="hidden" id="IsPersonalorFirm" name="IsPersonalorFirm">*@
                                            @*<input type="hidden" id="AgentUniqueId" name="AgentUniqueId">*@
                                            @Html.HiddenFor(m => m.AgentUniqueId, new { id = "AgentUniqueId" })
                                            @Html.HiddenFor(m => m.IsPersonalorFirm, new { id = "IsPersonalorFirm" })
                                            @*@Html.HiddenFor(m => m.MaritalStatusId, new { id = "hdn_MaritalStatusId" })
            @Html.HiddenFor(m => m.GenderId, new { id = "hdn_GenderId" })
            @Html.HiddenFor(m => m.NationalityId, new { id = "hdn_NationalityId" })
            @Html.HiddenFor(m => m.OccupationId, new { id = "hdn_OccupationId" })*@
                                    <div class="row gy-4">
                                        <div class="col-lg-4 col-sm-6">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label">User Name <strong class="text-danger">*</strong></label>

                                                    @Html.TextBoxFor(m => m.Username, new { @class = "form-control form-control-md" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-sm-6">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label">Password<strong class="text-danger">*</strong> </label>
                                                    <div class="form-control-wrap">
                                                        @{
                                                            <a href="javascript:void(0);" class="form-icon form-icon-right passcode-switch md" data-target="Password">
                                                                <em class="passcode-icon icon-show icon ni ni-eye"></em>
                                                                <em class="passcode-icon icon-hide icon ni ni-eye-off"></em>
                                                            </a>
                                                            @Html.TextBoxFor(m => m.Password, new { @class = "form-control form-control-md", @value = @ViewBag.Password })
                                                        }
                                                        <input type="hidden" id="hdnPassword" value="@ViewBag.Password" />

                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-sm-6" id="div_AgentCategoryId">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label">Category <strong class="text-danger">*</strong></label>
