                                                    @Html.HiddenFor(m => m.AgentCategory)
                                                    <span style="color:red" id="spanAgentCategoryId"></span>
                                                </div>
                                            </div>
                                        </div>



                                    </div>
                                            <br />
                                            <div class="row gy-4">
                                                <div class="col-lg-4 col-sm-6 tab">
                                                    <button type="button" class="tablinks active" id="Personaltab" style="float: left;  " onclick="IsPersonal(event, 'Personal')">Person</button>
                                                    <button type="button" class="tablinks" id="Firmtab" style="  " onclick="IsPersonal(event, 'Firm')"> Firm</button>
                                                </div>
                                            </div>
                                            <div class="cls_Personal" id="Personal" style="display:block;">
                                                <br />
                                                <br />


