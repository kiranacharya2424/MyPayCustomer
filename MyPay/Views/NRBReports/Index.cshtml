@model MyPay.Models.Add.AddNRBReports

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- content --s -->
<div class="nk-content ">
    <div class="container-fluid">
        <div class="nk-content-inner">
            <div class="nk-content-body">
                <div class="nk-block-head nk-block-head-sm">
                    <div class="nk-block-between">
                        <div class="nk-block-head-content">
                            <h3 class="nk-block-title page-title">NRB Reports</h3>
                        </div><!-- .nk-block-head-content -->
                        <div class="nk-block-head-content">
                            <div class="toggle-wrap nk-block-tools-toggle">
                                <a href="javascript:void(0);" class="btn btn-icon btn-trigger toggle-expand mr-n1" data-target="pageMenu"><em class="icon ni ni-menu-alt-r"></em></a>
                                <div class="toggle-expand-content" data-content="pageMenu">
                                    <ul class="nk-block-tools g-3">
                                        <li>
                                            <a href="javascript:void(0);" onclick="exported()" class="btn btn-white btn-dim btn-outline-primary"><em class="icon ni ni-download-cloud"></em><span>Export</span></a>
                                        </li>
                                    </ul>
                                </div>
                            </div><!-- .toggle-wrap -->
                        </div>
                    </div><!-- .nk-block-between -->
                </div><!-- .nk-block-head -->
                <div class="nk-block">
                    <div class="card card-bordered card-stretch">
                        <div class="card-inner-group">
                            <div class="card-inner position-relative card-tools-toggle pb-3 pt-3">
                                <!-- <span class="preview-title-lg overline-title mb-4">User List</span> -->
                                <div class="card-title-group">
                                    <div class="card-tools">
                                        <span class="preview-title-lg overline-title pb-0">NRB Reporting Data</span>
                                        <br />
                                        <div class="nk-block-des text-soft">
                                            <p>Below is NRB Reports displayed Annexture wise</p>
                                        </div>
                                    </div><!-- .card-tools -->
                                </div><!-- .card-title-group -->
                                <div class="nk-divider divider my-5"></div>
                                <div class="nk-block">
                                    <div class="row gy-4">
                                        <div class="col-lg-2 col-sm-6">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="TypeEnum">Annexture </label>
                                                    @Html.DropDownList("TypeEnumValue", (IEnumerable<SelectListItem>)ViewBag.Annexture , new { @id = "TypeEnum", @name = "TypeEnum", @class = "form-select form-control form-control-md" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-6 ">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="fromdate">Start Date</label>
                                                    <div class="form-icon form-icon-right md">
                                                        <em class="icon ni ni-calendar-alt"></em>
                                                    </div>
                                                    @Html.TextBox("FromDate", System.DateTime.UtcNow.AddMonths(-1).ToString("MM/dd/yyyy"), new { @id = "FromDate", @class = "form-control form-control-md form-control-outlined date-picker", @autocomplete = "off", @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-6 ">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="todate">End Date</label>
                                                    <div class="form-icon form-icon-right md">
                                                        <em class="icon ni ni-calendar-alt"></em>
                                                    </div>
                                                    @Html.TextBox("ToDate", System.DateTime.UtcNow.ToString("MM/dd/yyyy"), new { @id = "ToDate", @class = "form-control form-control-md form-control-outlined date-picker", @autocomplete = "off", @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-2 col-sm-6 NRBExtendedFilter">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="StartAmount">Start Amount</label>
                                                    @Html.TextBox("StartAmount", "0", new { @id = "StartAmount", @class = "form-control form-control-md", @maxlength = 10,  @onkeypress = "return isNumberKey(this, event);" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-6 NRBExtendedFilter">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="EndAmount">End Amount</label>
                                                    @Html.TextBox("EndAmount", "0", new { @id = "EndAmount", @class = "form-control form-control-md", @maxlength = 10, @onkeypress = "return isNumberKey(this, event);" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12 col-sm-12">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label"></label>
                                                    <a id="anchor_SearchReport" href="javascript:void(0);" onclick="return false;" class="btn btn-sm btn-success">  Search  </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                    </div>
                                    <div class="clearfix"></div>
                                    @*<div class="form-group">
                                        <div class="col-lg-4 col-sm-6">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <a id="anchor_SearchCommission" href="javascript:void(0);" onclick="return false;" class="btn btn-sm btn-success">  Search  </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>*@
                                    <div class="card-search search-wrap" data-search="search">
                                        <div class="card-body">
                                            <div class="search-content">
                                                <a href="javascript:void(0);" class="search-back btn btn-icon toggle-search" data-target="search"><em class="icon ni ni-arrow-left"></em></a>
                                                <input type="text" class="form-control border-transparent form-focus-none" placeholder="Search by user or email">
                                                <button class="search-submit btn btn-icon"><em class="icon ni ni-search"></em></button>
                                            </div>
                                        </div>
                                    </div>
                                </div><!-- .card-inner -->
                                <div id="dvMsgSuccess" class="tb-status text-success" style="text-align:center; font-weight:bold">
                                    @ViewBag.Message
                                </div>
                                <div id="dvMsg" class="tb-status text-danger" style="text-align: center; font-weight: bold">
                                    @ViewBag.SuccessMessage
                                </div>
                                <div class="table-responsive">
                                    <table id="tbllist5" class=" nk-tb-list nk-tb-ulist table table-striped" data-auto-responsive="false">
                                        <thead class="lg-th-head">
                                            <tr class="nk-tb-item nk-tb-head ">
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Form Of Transaction")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Number Upto 1000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Value Upto 1000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Number In 1000-5000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Value In 1000-5000")
                                                    </span>
                                                </th>


                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Number In 5000-10000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Value In 5000-10000")
                                                    </span>
                                                </th>
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Number In 10000-20000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Value In 10000-20000")
                                                    </span>
                                                </th>
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Number In 20000-25000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Value In 20000-25000")
                                                    </span>
                                                </th>
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Number Above 25000")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Value Above 25000")
                                                    </span>
                                                </th>
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Grand Total Number")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Grand Total Value")
                                                    </span>
                                                </th>

                                            </tr>
                                        </thead>
                                    </table>

                                    <!--- ANNEXTURE 6 -->

                                    <table id="tbllist6" class=" nk-tb-list nk-tb-ulist table table-striped" data-auto-responsive="false">
                                        <thead class="lg-th-head">
                                            <tr class="nk-tb-item nk-tb-head ">
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Form Of Transaction")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Success")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Failed")
                                                    </span>
                                                </th>
                                            </tr>
                                        </thead>
                                    </table>


                                    <!--- ANNEXTURE 8 -->

                                    <table id="tbllist8" class=" nk-tb-list nk-tb-ulist table table-striped" data-auto-responsive="false">
                                        <thead class="lg-th-head">
                                            <tr class="nk-tb-item nk-tb-head ">
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Wallet Type ")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("User Type")
                                                    </span>
                                                </th>

                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Total Number")
                                                    </span>
                                                </th>
                                                <th class="nk-tb-col min-100">
                                                    <span class="sub-text">
                                                        @Html.DisplayName("Total Balance")
                                                    </span>
                                                </th>
                                            </tr>
                                        </thead>
                                    </table>

                                </div><!-- .table-responsive -->
                            </div><!-- .card -->
                        </div><!-- .nk-block -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="//cdn.datatables.net/1.10.9/js/jquery.dataTables.min.js"></script>
<script src="~/content/assets/js/dtable_nrbreports.js"></script>
<script>
    function exported(e) {
        debugger;
        var TypeEnumId = $('#TypeEnum').val();
        var FromDate = $('#FromDate').val();
        var ToDate = $('#ToDate').val();
        var StartAmount = $('#StartAmount').val();
        var EndAmount = $('#EndAmount').val();
        var MemberId = "";
        var Type = "";
    $.ajax({
        type: "POST",
        url: '@Url.Action("ExportExcel", "NRBReports")', //call your controller and action
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        async: false,
        data: '{"Annexture":"' + TypeEnumId + '","MemberId":"' + MemberId + '","Type":"' + Type + '","StartAmount":"' + StartAmount + '","EndAmount":"' + EndAmount + '","StartDate": "' + FromDate + '", "EndDate": "' + ToDate + '"}',

    }).done(function (data) {
        //get the file name for download
        if (data.fileName != "") {
            //use window.location.href for redirect to download action for download the file
            window.location.href = "@Url.RouteUrl(new { Controller = "NRBReports", Action = "Download" })/?fileName=" + data.fileName;
        }
    });
}
</script>
 
